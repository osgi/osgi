# Set javac settings from JDT prefs
-include: ${build}/eclipse/jdt.bnd, ${build}/cmpn.bnd

Bundle-Activator: ${p}.Activator

-buildpath: \
	slf4j.api,\
	org.osgi.service.onem2m;version=latest,\
	org.osgi.framework;maven-scope=provided;version=1.8,\
	org.osgi.dto;maven-scope=provided;version=1.0,\
	org.osgi.util.promise;version=1.0,\
	org.osgi.util.function;version=1.0

-privatepackage     = \
    ${p}.*

