# Set javac settings from JDT prefs
-include: ${build}/eclipse/jdt.bnd

Bundle-Description					: A test case for the event generic mechanism implementation.

Conditional-Package					= org.osgi.test.support.*
Import-Package: ${-signaturetest}, *
Export-Package						= \
    org.osgi.test.support       , \
    org.osgi.test.support.compatibility        , \
	${p}.tbc.*

Include-Resource					= \
	tb1.jar, \
	tb2.jar, \
	tb3.jar, \
	tb4.jar, \
	tb5.jar

Test-Cases							= ${testcases}
-signaturetest                      = org.osgi.service.monitor

-buildpath							= \
    org.osgi.test.support;                  version=project , \
    org.osgi.service.monitor;               version=project , \
	osgi.core;								version=4.3.1	, \
	osgi.cmpn;								version=4.3.1	, \
	${junit}

-runbundles							= \
    ${junit},\
	org.osgi.impl.service.event; version=latest, \
    org.osgi.impl.service.cm; version=latest, \
	org.osgi.impl.service.dmt; version=latest, \
	org.osgi.impl.service.monitor; version=latest, \
	org.osgi.impl.service.log; version=latest



-runproperties = ${runsecureproperties}

