Bundle-Name:                             ${p}
Bundle-Version:                          1.0.0

-privatepackage:                          ${p}.impl


Require-Capability: osgi.extender;filter:="(osgi.extender=osgi.serviceloader.registrar)"
        
Provide-Capability:\
 osgi.serviceloader; osgi.serviceloader="${p}.spi.ColorProvider";\
   uses:="${p}.spi"; provider="CT"; .hint="by TIBCO"; type="one"; register:="org.osgi.test.cases.serviceloader.impl.ColorProviderImpl",\
 osgi.serviceloader; osgi.serviceloader="${p}.spi.ColorProvider";\
   uses:="${p}.spi"; provider="CT"; .hint="by TIBCO"; type="two"; register:="org.osgi.test.cases.serviceloader.impl.ColorProviderImpl2"
 

-includeresource = \
 {META-INF/services/${p}.spi.ColorProvider=resources/spi/ColorProvider}

