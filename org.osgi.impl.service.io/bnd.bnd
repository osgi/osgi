# Set javac settings from JDT prefs
-include: ${build}/eclipse/jdt.bnd

Bundle-Activator				= ${p}.Activator
Bundle-Description				= OSGi ConnectionService 1.0 Reference Implementation by ProSyst AG.

-privatepackage 				= ${p}.*
Export-Package					= org.osgi.service.io; -split-package:=first; provide:=true

-buildpath						= \
    org.osgi.service.io;				version=project, \
    osgi.core;                      version=4.3.1, \
    osgi.cmpn;                      version=4.3.1
