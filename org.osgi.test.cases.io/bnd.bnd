Bundle-Description: Tests the IO Service
Bundle-Version: 4.2.0.${project.build}

Private-Package						= org.osgi.test.support.*, \
	${p}.junit.*, \
	javax.microedition.io, \
	javax.microedition.pki, \
	
Import-Package: javax.microedition.io, javax.microedition.pki, *
Export-Package: javax.microedition.io, javax.microedition.pki
	
Test-Cases							= ${testcases}
-signaturetest                      = org.osgi.service.io

-buildpath							= \
    org.osgi.test.support;                  version=project , \
    org.osgi.service.io;                  	version=project , \
	osgi.core;								version=4.0	, \
	osgi.cmpn;								version=4.0	, \
	ee.foundation;							version=1.1		, \
	${junit}

-runbundles							= \
	osgi.cmpn; version=${cmpn.published.version};strategy=lowest, \
	org.osgi.impl.service.io; version=project


-runproperties = ${runproperties}
